<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--Aware-->
	<bean id="myApplicationAware" class="org.springframework.test.yly.aware.MyApplicationAware"></bean>

	<!--BeanPostProcessor-->
	<bean id="beanPostProcessorTest" class="org.springframework.test.yly.beanpostprocessor.BeanPostProcessorTest "></bean>

	<!--InitializingBean-->
	<bean id="initializingBeanTest" class="org.springframework.test.yly.init.InitializingBeanTest">
		<property name="name" value="chenssy 1 号"/>
	</bean>

	<!--init-method-->
	<bean id="initMethodBeanTest" class="org.springframework.test.yly.init.InitMethodBeanTest"
		  init-method="setOtherName">
		<property name="name" value="chenssy 1 号"/>
	</bean>

</beans>